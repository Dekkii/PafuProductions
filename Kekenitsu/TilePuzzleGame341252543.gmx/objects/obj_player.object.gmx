//check for win and dead



//MOVE--------------------------------
//get keypress
if keyboard_check(vk_up) {dir = 0}
if keyboard_check(vk_right) {dir = 1}
if keyboard_check(vk_down) {dir = 2}
if keyboard_check(vk_left) {dir = 3}
if keyboard_check(vk_backspace) {room_goto_next()}

if keyboard_check(vk_space)
{
    if win
    {
        room_goto_next()
    }
    
    else if dead
    {
        room_speed = global.gameSpeed
        room_restart()
    }
}


//reset button
if keyboard_check(ord('R')) {room_restart()}




//boulder check
with obj_boulder
{
    if objChk((obj_player.dir+2)%4,obj_player)
    {
        if objChk(obj_player.dir,obj_water)
        {
            objDel(obj_player.dir,obj_water)
            instance_destroy()
            audio_play_sound(snd_splash,1,0)
        }
        moveChk(obj_player.dir)
    }
}

//key check
with obj_key
{
    if objChk((obj_player.dir+2)%4,obj_player)
    {
        instance_destroy()
    }
}

//fake wall check
with obj_fakewalls
{
    if objChk((obj_player.dir+2)%4,obj_player)
    {
        image_index += 1
        if image_index &gt; 1
        {
            instance_destroy()
            audio_play_sound(snd_break,1,0)
        }
        else
        {
            audio_play_sound(snd_hit,1,0)
        }
    }
}

//dustcheck
with obj_dust
{
    if objChk((obj_player.dir+2)%4,obj_player)
    {
        instance_destroy()
    }
}


//actually move
if !win &amp;&amp; !dead
{
    image_index += 1
    moveChk(dir)
}
dir = noone
